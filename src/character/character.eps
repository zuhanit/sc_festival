const characters = EUDArray(16);
var characterCount = 0;

object Character {
    var name;
    var unit: TrgUnit;
    var description;
    var skillFuncPtr;
};

function registerCharacter(name, unit:TrgUnit, skillFuncPtr: EUDFuncPtr(0, 0), description) {
    const obj = Character.alloc();
    obj.name = name;
    obj.unit = unit;
    obj.description = description;
    obj.skillFuncPtr = skillFuncPtr;
    characters[characterCount] = obj;
    characterCount++;
}

