import player.skill;
import player.player;

function skill(pObj, psObj, cObj) {
    const cp = getcurpl();
    const obj = skill.Skill.cast(player.playersSkill[cp]);
    if (obj.using() == 0) {
        if (obj.read() == 1) {
            obj.dequeue();
            DisplayText("Hello!");
        }
    }
}